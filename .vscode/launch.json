    "version": "0.2.0",
    "configurations": [
        {
            "name": "CFN Submit",
            "type": "python",
            "request": "launch",
            "program": ".env/bin",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "envFile": "${workspaceFolder}/.env",
            "console": "integratedTerminal",
            "args": [
                "submit",
                "--dry-run"
            ],
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
    ]
}